                            {
                                "ru": "\u0417\u043e\u043b\u043e\u0442\u043e\u0435 \u0431\u043e\u0436\u0435\u0441\u0442\u0432\u043e"
                            },
                            {
                                "th": "\u05e7\u05d0\u05de\u05d5\u05d9 \u05de\u05d5\u05d6\u05d4\u05d1"
